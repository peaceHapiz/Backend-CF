enum Type { 
  Eksternal
  Internal
  Guru
  Keluarga_Siswa
  Alumni
}

model TicketOffline {
  id          Int      @id @default(autoincrement())
  isReady     Boolean
  productId   Int      @unique
  alias       String   @unique
  name        String
  price       Int


  ticket_type    Type @default(Eksternal)
  unit        String   @default("IDR")
  unitName    String   @default("Rupiah")
  unitSymbol  String   @default("Rp")

   transactions TicketTransaction[]
}
